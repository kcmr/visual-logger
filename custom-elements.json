{
  "version": "experimental",
  "tags": [
    {
      "name": "visual-logger",
      "path": "./src/VisualLogger.js",
      "description": "Browser UI terminal using Xterm.js to display console methods (log, warn, error, info).",
      "attributes": [
        {
          "name": "no-console",
          "description": "Supress window.console logs",
          "type": "boolean"
        },
        {
          "name": "cols",
          "description": "Terminal columns",
          "type": "number",
          "default": "200"
        },
        {
          "name": "rows",
          "description": "Terminal rows",
          "type": "number",
          "default": "5"
        },
        {
          "name": "line-height",
          "description": "Line height",
          "type": "number",
          "default": "1.5"
        },
        {
          "name": "excluded-log-methods",
          "description": "Excluded log (window.console) methods",
          "type": "array",
          "default": "[]"
        },
        {
          "name": "stylesheet-uri",
          "description": "URI of the Terminal stylesheet (Uses cdnjs if not provided)",
          "type": "string",
          "default": "\"https://cdnjs.cloudflare.com/ajax/libs/xterm/3.14.5/xterm.min.css\""
        }
      ],
      "properties": [
        {
          "name": "theme",
          "description": "Terminal theme",
          "type": "object"
        },
        {
          "name": "noConsole",
          "attribute": "no-console",
          "description": "Supress window.console logs",
          "type": "boolean"
        },
        {
          "name": "terminal",
          "description": "Returns the xterm intance"
        },
        {
          "name": "cols",
          "attribute": "cols",
          "description": "Terminal columns",
          "type": "number",
          "default": "200"
        },
        {
          "name": "rows",
          "attribute": "rows",
          "description": "Terminal rows",
          "type": "number",
          "default": "5"
        },
        {
          "name": "lineHeight",
          "attribute": "line-height",
          "description": "Line height",
          "type": "number",
          "default": "1.5"
        },
        {
          "name": "excludedLogMethods",
          "attribute": "excluded-log-methods",
          "description": "Excluded log (window.console) methods",
          "type": "array",
          "default": "[]"
        },
        {
          "name": "stylesheetUri",
          "attribute": "stylesheet-uri",
          "description": "URI of the Terminal stylesheet (Uses cdnjs if not provided)",
          "type": "string",
          "default": "\"https://cdnjs.cloudflare.com/ajax/libs/xterm/3.14.5/xterm.min.css\""
        }
      ]
    }
  ]
}